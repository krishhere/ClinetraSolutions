<!DOCTYPE html>
<html lang="en">
<head>
    <title>Clinetra solutions - Admin Panel</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
    <script>
        window.onload = function () {
            window.history.forward();
            setTimeout('window.history.forward();', 500);
        };
    </script>
</head>
<body class="dark">
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
        <div class="container">
            <a class="navbar-brand" href="@Url.Action("Index", "AdminMain")">Admin Panel</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav">
                    <li class="nav-item"><a class="nav-link" href="@Url.Action("Index", "AdminMain")" style="padding-right:0px;">Home |</a></li>
                    <li class="nav-item"><a class="nav-link" href="@Url.Action("", "AdminMain")" style="padding-right:0px;">Conferences |</a></li>
                </ul>
                <ul class="navbar-nav ms-auto">
                    <li class="nav-item"><a class="nav-link text-danger" href="@Url.Action("Logout", "AdminMain")">Logout</a></li>
                </ul>
            </div>
        </div>
    </nav>
    <div class="container mt-2">
        @RenderBody()
    </div>
</body>
</html>